{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./client-app/**", "./server/**"]
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    },
    "lint": {

    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}